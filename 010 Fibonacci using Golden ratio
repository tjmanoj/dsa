#include<bits/stdc++.h>
using namespace std;
 
//Note: Use double to store the golden ratio to work correctly and use the round function to get the precise answer

int fibo(int n){
     double phi = (1 + sqrt(5)) / 2;
     return round(pow(phi, n)/ sqrt(5));
 }
 
int main() {
  cout << fibo(4);
  return 0;
}
