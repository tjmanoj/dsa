
#include<bits/stdc++.h>
using namespace std;

// Optimal 1
TC  : O(N)
SC  : O(N/2)
int usingMap(int a[], int n){
    unordered_map<int,int> m;
    for(int i=0;i<n;i++)
        m[a[i]]++;
    for(auto i: m) if(i.second == 1) return i.first;
    return -1;
}


//BEST OPTIMAL
//TC  : O(N)
  SC  : O(1)
int singleEl(int a[], int n){
    int result = 0;
    for(int i=0;i<n;i++) result ^= a[i];
    return result;
}

int main() {
  int a[9] = {3,5,9,4,9,6,5,3,4};
  cout << singleEl(a, 9);
  return 0;
}
